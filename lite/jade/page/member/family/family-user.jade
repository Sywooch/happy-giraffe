    mixin familySettings(mod, vars)

        // family-settings
        .family-settings.clearfix(class=mod)
            .family-edit
                if(vars.icoEditTop)
                    a.ico-edit.ico-edit__s(href='#')
                a.ico-close4(href='#')

            if(vars.type == 'child')
                .family-settings_t Дети
            .family-settings_hold.clearfix1
                .family-settings_l
                    .family-settings_photo
                        if(vars.photoCapAdd)
                            // Блок Добавления фото
                            .cap-add.cap-add__i-photo
                                .cap-add_hold
                                    .cap-add_in 
                                        .cap-add_tx Добавить фото 
                    
                        if(vars.photo)
                            +iPhoto("", iPhoto.familySettings)
                        if(vars.photoIco)
                            +iPhoto("", iPhoto.familySettingsIco)
                        if(vars.photoEdit)
                            +iPhoto("", iPhoto.familySettingsEdit)
                    
                .family-settings_r
                    // gender
                    if(vars.genderCap)
                        // Пол
                        .family-settings_row
                            a.family-settings_cap.family-settings_cap__gender(href='#') Сын или дочь?
                    if(vars.gender)
                        // Пол
                        .family-settings_row
                            .family-settings_tx
                                | Сын
                                if(vars.icoEditLine)
                                    a.ico-edit.ico-edit__s(href='#')
                    if(vars.genderEdit)
                        // Пол
                        .family-settings_row
                            // В случае ошибки .error на .inp-valid
                            .inp-valid.inp-valid__abs 
                                select.select-cus.select-cus__gray.select-cus__search-off(placeholder="Выберите пол")
                                    option
                                    option(value="1") Дочь
                                    option(value="2") Сын
                            a.btn.btn-success.btn-xs(href="#") ok

                    // name
                    if(vars.nameCap)
                        // Пол
                        .family-settings_row
                            a.family-settings_cap.family-settings_cap__name(href='#') Имя ребенка
                    if(vars.nameEdit)
                        .family-settings_row
                            //a.family-settings_cap.family-settings_cap__name(href='#') Имя ребенка
                            .inp-valid.inp-valid__abs
                                input.itx-gray(type='text')
                            a.btn.btn-success.btn-xs(href="#") ok
                    if(vars.name)
                        .family-settings_row
                            .family-settings_name
                                | Кирилл
                                if(vars.icoEditLine)
                                    a.ico-edit.ico-edit__s(href='#')
                    // date
                    if(vars.dateCap)
                        // Дата
                        .family-settings_row
                            a.family-settings_cap.family-settings_cap__date(href='#') Дата рождения
                    if(vars.dateEdit)
                        // Дата
                        .family-settings_row
                            // .family-settings_cap__name
                            // .family-settings_cap__date
                            // .family-settings_cap__desc
                            //a.family-settings_cap.family-settings_cap__date(href='#') Дата рождения
                            .inp-valid.inp-valid__abs.error
                                .float-l.w-80.margin-r10
                                    select.select-cus.select-cus__search-off.select-cus__gray(placeholder="День")
                                        option
                                        option(value="1") 1
                                        option(value="2") 2
                                        option(value="3") 3
                                        option(value="4") 4
                                        option(value="5") 5
                                        option(value="6") 6
                                        option(value="7") 7
                                        option(value="8") 8
                                        option(value="8") 9
                                        option(value="10") 10
                                        option(value="11") 11
                                        option(value="12") 12
                                .float-l.w-135.margin-r10
                                    select.select-cus.select-cus__search-off.select-cus__gray(placeholder="Месяц")
                                        option
                                        option(value="1") Января
                                        option(value="2") Февраля
                                        option(value="3") Марта
                                        option(value="4") Апреля
                                        option(value="5") Майя
                                        option(value="6") Июня
                                        option(value="7") Июля
                                        option(value="8") Августа
                                        option(value="9") Сентября
                                        option(value="10") Октября
                                        option(value="11") Ноября
                                        option(value="12") Декабря
                                .float-l.w-80
                                    select.select-cus.select-cus__search-off.select-cus__gray(placeholder="Год")
                                        option
                                        option(value="1") 1912
                                        option(value="2") 1913
                                        option(value="3") 1914
                                        option(value="4") 1915
                                        option(value="5") 1916
                                        option(value="6") 1917
                                        option(value="7") 1918
                                        option(value="8") 1919
                                        option(value="8") 1920
                                        option(value="10") 1921
                                        option(value="11") 1922
                                        option(value="12") 1923
                                
                                .inp-valid_error
                                    .errorMessage 
                                        | Не выбрана дата
                            a.btn.btn-success.btn-xs(href="#") ok
                    if(vars.date)
                        // Дата
                        .family-settings_row
                            .family-settings_tx
                                | 1 год 6 месяцев
                                if(vars.icoEditLine)
                                    a.ico-edit.ico-edit__s(href='#')
                    // desc
                    if(vars.descCap)
                        .family-settings_row

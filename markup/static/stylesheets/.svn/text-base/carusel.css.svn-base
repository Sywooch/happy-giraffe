.next, .prev {z-index: 99; position: absolute; background: url(../images/button-green.png) no-repeat; text-decoration: none;}
.jCarousel{ position: relative; padding: 0 20px; }
.jCarousel .next, .jCarousel .prev { width: 19px; height: 19px; bottom: 90px; display: block;}
.jCarousel .prev{ background-position: 0 -1218px; left: 0px}
.jCarousel .next  { background-position: -20px -1218px;right: 0px }
.jCarousel .prev.disabled{ background-position: 0 -1200px; left: 0px}
.jCarousel .next.disabled  { background-position: -20px -1200px;right: 0px }

.inner.contest .one_photo .photo-content.jCarousel2 { position: relative; padding: 00px 30px 0px; }
.inner.contest .one_photo .photo-content.jCarousel2 .item { padding: 30px 0 120px; position: relative;}
.jCarousel2 .next, .jCarousel2 .prev { width: 29px; height: 39px; bottom: 290px; display: block;}
.jCarousel2 .prev{ background-position: 0 -1158px; left: 0px}
.jCarousel2 .next  { background-position: -30px -1158px;right: 0px }
.jCarousel2 .prev.disabled{ background-position: 0 -1105px; left: 0px}
.jCarousel2 .next.disabled  { background-position: -30px -1105px;right: 0px }
.paginator {color: #ababab;  
   position: relative; width: 100%;font:11px 'AndaleMono',Courier, Courier New, monospace;
   margin: 20px 0; 
 }
.paginator .txt { margin: 0 30px 0 60px; display: block; float: left;
}
.paginator .pages { display: block; float: left;}
.paginator .pages .txt { float: left; margin: 0 10px 0 0}
.paginator .pages a.next, .paginator .pages a.prev {
   background: url(../images/button-green.png) no-repeat;    
   display: block;
   width: 20px;
   height: 20px;
   float: left;
}
.paginator .pages a.prev { background-position: left -365px; margin: -5px 0 0}
.paginator .pages a.next { background-position: right -365px; margin: -5px 0 0}

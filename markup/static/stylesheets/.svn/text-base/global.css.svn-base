@charset "utf-8";
/* CSS Document */


html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {
margin: 0;
padding: 0;
border: 0;
outline: 0;
font-size: 100%;
vertical-align: baseline;
background: transparent;
}
body {
line-height: 1;
}
ol, ul {
list-style: none;
}
blockquote, q {
quotes: none;
}
.overlay { position: absolute; width: 100%; height: 100%; color: #000;  opacity:0.5; top:0; left: 0;
/*top:-10px; left:-245px;  background: url(../images/overlay.png) no-repeat; width: 1400px; height: 5000px;
*/
}

/* remember to define focus styles! */
:focus {
outline: 0;
}

/* remember to highlight inserts somehow! */
ins {
text-decoration: none;
}
del {
text-decoration: line-through;
}
input { border: none; background: none;}
/* tables still need 'cellspacing="0"' in the markup */
table {
border-collapse: collapse;
border-spacing: 0;
}
html, body {
    min-height: 100%;
    margin: 0;
    padding: 0;
}

body { position: relative;
background: #EEDFFD;
font:12px Arial, Tahoma, sans-serif; 
font-weight: normal;
color: #1f1f1f; 
text-decoration: none;
}
html > body .page {
    height: auto;
    min-height: 100%;
    position: relative;
}
.clear { width: 99%; clear: both;}
h1 { font-size: 30px;}
h2 { font-size: 26px;}
h3 { font-size: 16px;}
h4 { font-size: 12px}
p { font-size: 11px; line-height: 17px; color: #1f1f1f}
.g-button { height: 34px; padding: 0 6px; position: relative; display: inline-block;}
.g-button .fill, .g-button .left, .g-button .right {
   background: url(../images/button-green.png) no-repeat; 
     
}
.g-button .fill { background-repeat: repeat-x; text-transform: uppercase;}
.g-button .left, .g-button .right{ position: absolute; top:0; width: 6px; height: 100%; padding: 0}
.g-button .left { background-position: left -35px; left: 0}
.g-button .right { background-position: right -69px; right: 0;}
.g-button .fill input { width: auto; background: none; padding:9px 5px 9px; color: #f3f083; 
font: bold 12px Arial, Helvetica, sans-serif; text-transform: uppercase;}

.g-button.yellow { height: 30px; padding: 0 5px; position: relative; display: inline-block;}
.g-button.yellow .fill { background-repeat: repeat-x; background-position: 0 -103px; padding: 5px 0 4px; }
.g-button.yellow .left { background-position: left -125px; }
.g-button.yellow .right { background-position: right -149px; }
.g-button.yellow .fill a { width: auto; background: none; padding:0 5px; color: #914b21; 
font: bold 10px Arial, Helvetica, sans-serif; text-decoration: none;}

.g-button.round { height: 40px; padding: 0 12px; position: relative; display: inline-block;}
.g-button.round .fill { background-repeat: repeat-x; background-position: 0 -173px; padding: 8px 0 8px; }
.g-button.round .left { background-position: left -212px; width: 12px;}
.g-button.round .right { background-position: right -250px; width: 12px; }
.g-button.round .fill a { width: auto;  padding:0 25px 0 15px; color: #fff; 
font: 20px "OfficinaSansBookC",Tahoma, Arial, Helvetica, sans-serif; text-decoration: none; text-transform: capitalize;}

.g-button.blue { height: 17px; padding: 0 5px; position: relative; display: inline-block; text-decoration: none;}
.g-button.blue .left { background-position: left -409px; width: 5px;}
.g-button.blue .right { background-position: right -426px; width: 5px; }
.g-button.blue .fill {background-repeat: repeat-x; 
background-position: 0 -391px; 
height: 17px; padding: 0 5px; 
display:block; color: #fff; 
font:11px/18px 'AndaleMono',Courier, Courier New, monospace; 
text-decoration: none; 
text-transform: capitalize;
}

.g-button.orange { height: 34px; padding: 0 11px; position: relative; display: inline-block; text-decoration: none;}
.g-button.orange .left { background-position: left -481px; width: 11px;}
.g-button.orange .right { background-position: right -513px; width: 11px; }
.g-button.orange .fill {background-repeat: repeat-x; 
background-position: 0 -447px; 
height: 17px; padding: 3px 5px 12px; 
display:block; color: #fff; 
font: 14px/25px "OfficinaSansBookC",Tahoma, Arial, Helvetica, sans-serif; 
text-decoration: none; 
text-transform: none;
}

.g-button.grey { height: 34px; padding: 0 7px; position: relative; display: inline-block; text-decoration: none;}
.g-button.grey .left { background-position: left -583px; width: 7px;}
.g-button.grey .right { background-position: right -617px; width: 7px; }
.g-button.grey .fill {background-repeat: repeat-x; 
background-position: 0 -547px; 
height: 17px; padding: 5px 5px 12px; 
display:block; color: #fff; 
font: 14px/25px "OfficinaSansBookC",Tahoma, Arial, Helvetica, sans-serif; 
text-decoration: none; 
text-transform: uppercase;
}

.g-button.red .fill { background-repeat: repeat-x; text-transform: uppercase; background-position: left -677px;}
.g-button.red .left, .g-button .right{ position: absolute; top:0; width: 6px; height: 100%; padding: 0}
.g-button.red .left { background-position: left -714px; left: 0}
.g-button.red .right { background-position: right -748px; right: 0;}
.g-button.red .fill input { width: auto; background: none; padding:9px 5px 9px; color: #fff; 
font: 14px/25px "OfficinaSansBookC",Tahoma, Arial, Helvetica, sans-serif;  text-transform: uppercase;}

.g-button.yellow-big { height: 34px; padding: 0 6px; position: relative; display: inline-block; text-decoration: none;}
.g-button.yellow-big .left { background-position: left -819px; width: 6px;}
.g-button.yellow-big .right { background-position: right -853px; width: 6px; }
.g-button.yellow-big .fill {background-repeat: repeat-x; 
background-position: 0 -783px; 
height: 17px; padding: 5px 5px 12px; 
display:block; color: #8b5909; 
font: 14px/25px "OfficinaSansBookC",Tahoma, Arial, Helvetica, sans-serif; 
text-decoration: none; 
text-transform: uppercase;
}

.g-button.small {float: left; height: 20px; padding: 0 7px; position: relative; display: block; text-decoration: none;}
.g-button.small .left { background-position: left -912px; width: 7px;}
.g-button.small .right { background-position: right -931px; width: 7px; }
.g-button.small .fill {background-repeat: repeat-x; 
background-position: 0 -888px; 
height: 12px; padding: 3px 0px 5px; 
display:block; color: #fff; 
font: bold 10px Arial, Helvetica, sans-serif; 
text-decoration: none; 
text-transform: uppercase;
}
.g-button.photo-name {float: left; height: 42px; padding: 0 7px 0 15px; position: relative; display: block; text-decoration: none;}
.g-button.photo-name .left { background-position: left -1033px; width: 15px;}
.g-button.photo-name .right { background-position: right -1069px; width: 7px; }
.g-button.photo-name .fill {background-repeat: repeat-x; 
background-position: 0 -996px; 
min-height: 22px; padding: 6px 0px 5px; 
display:block; color: #027089; 
font: 11px 'AndaleMono',Courier, Courier New, monospace; 
text-decoration: none; 
text-transform: none;
text-align: left;
}
